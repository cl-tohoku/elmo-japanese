{
  "bidirectional": true,
  "char_cnn": {
    "activation": "relu",
    "embedding": {"dim": 16},
    "filters": [[1, 4],
                [2, 4],
                [3, 8],
                [4, 8]],
    "max_characters_per_token": 8,
    "n_characters": 0,
    "n_highway": 2
  },
  "dropout": 0.1,
  "lstm": {
    "cell_clip": 3,
    "dim": 50,
    "n_layers": 2,
    "proj_clip": 3,
    "projection_dim": 20,
    "use_skip_connections": true
  },
  "all_clip_norm_val": 10.0,
  "n_epochs": 50,
  "n_train_tokens": 0,
  "batch_size": 8,
  "n_tokens_vocab": 0,
  "unroll_steps": 10,
  "n_negative_samples_batch": 100
}
